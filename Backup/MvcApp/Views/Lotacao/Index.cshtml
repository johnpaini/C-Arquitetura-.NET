@model IEnumerable<Treinamento2013Curso03.ModelData.Logic.Curso2013Turma03.Model.Lotacao>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table>
    <tr>
        <th>
            DescricaoLotacao
        </th>
        <th>
            DataInsert
        </th>
        <th>
            DataUpdate
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DescricaoLotacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataInsert)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataUpdate)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.IDLotacao })
        </td>
    </tr>
}

</table>
